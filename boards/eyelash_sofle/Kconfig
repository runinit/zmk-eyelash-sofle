config BOARD_EYELASH_SOFLE_LEFT
    select BOARD_EYELASH_SOFLE

config BOARD_EYELASH_SOFLE_RIGHT
    select BOARD_EYELASH_SOFLE

config BOARD_EYELASH_SOFLE
    bool
    select SOC_SERIES_NRF52X
    select SOC_NRF52840

config BOARD_EYELASH_SOFLE_LEFT
    bool
    default y if BOARD_EYELASH_SOFLE_NRF52840_LEFT
    select BOARD_EYELASH_SOFLE

config BOARD_EYELASH_SOFLE_RIGHT
    bool
    default y if BOARD_EYELASH_SOFLE_NRF52840_RIGHT
    select BOARD_EYELASH_SOFLE

rsource "Kconfig.defconfig"
